{
  "name": "custom-code-insertion",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "cmp-s": "source schematic/cmp.sh static",
    "cmp-b": "source schematic/cmp.sh base",
    "cmp-g": "source schematic/cmp.sh group",
    "cmp-w": "source schematic/cmp.sh widget",
    "cmp-c": "source schematic/cmp.sh structural",
    "cmp-v": "source schematic/cmp.sh view",
    "sb": "gulp build-copy && gulp build-css; gulp watch --files=css & npx start-storybook --ci -p 9001 -s ./dist/build/",
    "esdoc": "./node_modules/.bin/esdoc",
    "typedoc": "./node_modules/typedoc/bin/typedoc src",
    "dir-tree": "./node_modules/.bin/tree -l 10 --ignore 'node_modules, doc, coverage'",
    "schema": "./node_modules/.bin/typescript-json-schema 'src/model/rule/index.ts' Rules --required --out src/model/rule/schema.json",
    "ts": "npx webpack --mode=production",
    "ts:dev": "npx webpack",
    "test": "npx jest --detectOpenHandles",
    "test-generate-cov": "npm run test -- --collectCov=true",
    "test-collect-cov": "cat ./doc/test-report/lcov.info | coveralls",
    "build": "npm run schema && npm run test && npx gulp build --prod && npm run ts && npx gulp zip --prod",
    "build:dev": "npm run schema && npm run test && npx gulp build && npm run ts:dev"
  },
  "private": true,
  "dependencies": {
    "codemirror": "5.58.2",
    "jsonschema": "1.4.0",
    "pubsub-js": "1.9.0",
    "react": "16.13.1",
